{
    "name": "dnadesign/silverstripe-populate",
    "description": "Populate your database through YAML files",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "yaml", "fixture", "populate", "mock", "faker"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Will Rossiter",
        "email": "will@fullscreen.io"
    }],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^5",
        "silverstripe/versioned": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DNADesign\\Populate\\": "code/",
            "DNADesign\\Populate\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
